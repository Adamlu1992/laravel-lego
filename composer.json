{
    "name": "wutongwan/lego",
    "description": "Yet another CRUD widgets for laravel 5.x",
    "homepage": "https://github.com/wutongwan/laravel-lego",
    "license": "MIT",
    "keywords": [
        "crud",
        "admin",
        "widget"
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "5.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "files": [
            "src/Lego/Helper/functions.php"
        ],
        "psr-4": {
            "Lego\\": "src/Lego/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}